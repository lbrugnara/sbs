preset win-cygwin-clang-x64-debug {
    env: win-cygwin-x64,
    toolchain: clang,
    config: clang-win-debug,
    target: sbs-objects,
    actions: {
        before: [
            `echo 'Running preset win-cygwin-x64'`,
            rm-objects
        ],
        after: [
            `echo 'Finished running preset win-cygwin-x64'`
        ]
    },
}

preset win-cygwin-clang-x64-release {
    env: win-cygwin-x64,
    toolchain: clang,
    config: clang-win-release,
    target: sbs-objects,
    actions: {
        before: [
            `echo 'Running preset win-cygwin-x64'`,
            rm-objects
        ],
        after: [
            `echo 'Finished running preset win-cygwin-x64'`
        ]
    },
}
