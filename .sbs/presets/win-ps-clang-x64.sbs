preset win-ps-clang-x64-debug {
    env: win-ps-x64,
    toolchain: clang,
    config: clang-win-debug,
    target: sbs-objects,
    actions: {
        before: [
            `echo 'Running preset win-ps-x64'`,
            rm-objects
        ],
        after: [
            `echo 'Finished running preset win-ps-x64'`
        ]
    },
}

preset win-ps-clang-x64-release {
    env: win-ps-x64,
    toolchain: clang,
    config: clang-win-release,
    target: sbs-objects,
    actions: {
        before: [
            `echo 'Running preset win-ps-x64'`,
            rm-objects
        ],
        after: [
            `echo 'Finished running preset win-ps-x64'`
        ]
    },
}