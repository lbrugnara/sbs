toolchain cl {
    for win-cmd-x64, win-cmd-x86, win-ps-x64, win-ps-x86 {
        compiler: {
            bin: "cl",
            include_dir_flag: "/I",
            define_flag: "/D"
        }
        archiver: {
            bin: "lib"
        }
        linker: {
            bin: "link",
            lib_dir_flag: "/LIBPATH:",
            lib_flag: "/LIB:"
        }
    }
}
