env win-wsl-x64 {
    type: bash,
    terminal: 'wsl.exe bash',
    args: [ ],
    variables: [
        "WSLENV=MY_ENV_VAR/u", # https:#devblogs.microsoft.com/commandline/share-environment-vars-between-wsl-and-windows/
    ],
    # Scripts to run before doing anything
    actions: {
        before: [
            `PATH=/mnt/d/dev/LLVM/x64/bin:$PATH`,
        ]
    }
}